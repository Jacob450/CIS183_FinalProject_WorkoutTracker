<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AppendSession">

    <TextView
        android:id="@+id/tv_alcc_addrep2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:outlineProvider="paddedBounds"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView25"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/sp_aps_liftTypes"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:outlineProvider="paddedBounds"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_aps_reps"
        app:layout_constraintEnd_toStartOf="@+id/textView26"
        app:layout_constraintStart_toStartOf="@+id/textView27" />

    <EditText
        android:id="@+id/et_aps_reps"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginStart="220dp"
        android:backgroundTint="#D0BCFF"
        android:ems="10"
        android:inputType="number"
        android:outlineProvider="paddedBounds"
        android:textColor="#D0BCFF"
        android:textColorHighlight="#F60808"
        android:textColorHint="#000000"
        app:layout_constraintEnd_toStartOf="@+id/et_aps_weight"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView28" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="51dp"
        android:text="Weight"
        android:textColor="#D0BCFF"
        android:textColorLink="#D0BCFF"
        app:layout_constraintStart_toStartOf="@+id/et_aps_weight"
        app:layout_constraintTop_toBottomOf="@+id/textView25" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="76dp"
        android:text="Lift Type"
        android:textColor="#D0BCFF"
        android:textColorLink="#D0BCFF"
        app:layout_constraintBottom_toTopOf="@+id/sp_aps_liftTypes"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:text="Reps"
        android:textColor="#D0BCFF"
        android:textColorLink="#D0BCFF"
        app:layout_constraintBottom_toTopOf="@+id/sp_aps_liftTypes"
        app:layout_constraintStart_toStartOf="@+id/et_aps_reps" />

    <EditText
        android:id="@+id/et_aps_weight"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginEnd="66dp"
        android:backgroundTint="#D0BCFF"
        android:ems="10"
        android:inputType="number"
        android:outlineProvider="paddedBounds"
        android:textColor="#D0BCFF"
        android:textColorHighlight="#F60808"
        android:textColorHint="#000000"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_aps_reps"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/et_aps_reps" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/audiowide"
        android:text="AddLift"
        android:textColor="#D0BCFF"
        android:textSize="34sp"
        app:layout_constraintStart_toEndOf="@+id/textView27"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_aps_add"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginBottom="41dp"
        android:text="Add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_aps_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="41dp"
        android:text="back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>